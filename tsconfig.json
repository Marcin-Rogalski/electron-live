{
	"compilerOptions": {
		"moduleResolution": "Node",
		"target": "ESNext",
		"lib": [ "ESNext", "ES5", "ES6", "ES2015", "ES2016", "ES2017", "ES2018", "ES2019", "DOM" ],
		"module": "CommonJS",
		"allowJs": true,
		"esModuleInterop": true,
